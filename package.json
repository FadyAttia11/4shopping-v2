{
  "name": "mernboilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "server/App.js",
  "engine": {
    "node": "13.3.0",
    "npm": "6.13.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server/App",
    "client": "cd client && npm run start",
    "server": "nodemon server/index --ignore client/",
    "client-install": "cd client && npm install",
    "server-install": "npm install",
    "install-all": "concurrently \"npm run client-install\" \"npm run server-install\"",
    "all": "concurrently \"npm run server\" \"npm run start  --prefix client\""
  },
  "author": "FadyAttia",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.3.0",
    "bcrypt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.2",
    "mongoose": "^5.9.12",
    "multer": "^1.4.2",
    "sharp": "^0.25.4",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "env-cmd": "^10.1.0",
    "http-proxy-middleware": "^1.0.6",
    "nodemon": "^2.0.6",
    "supertest": "^4.0.2"
  }
}
